# ----------  linear search---------------


def linear_search(arr,target):
     for value in range(len(arr)):
          if arr[value] == target:
             return value
     return False

lst=[12,45,2,3,6,5,8,9,78,52,63]
key=58
result=linear_search(lst,key)

if result:
    print("element found at index:",result);
else:
    print("element not found in array")
